{
  "name": "demo",
  // 自定义任务名称
  "settings": {
    "speed": {
      "channel": 1,
      "bytes": 0
    },
    "errorLimit": {
      "record": 100
    },
    "restore": {
      "maxRowNumForCheckpoint": 0,
      "isRestore": false,
      "restoreColumnName": "",
      "restoreColumnIndex": 0
    },
    "log": {
      "isLogger": false,
      "level": "debug",
      "path": "",
      "pattern": ""
    }
  },
  // 数据流 Source -> Sink. 可以启动多个
  "data_stream": [
    {
      "source": {
        // 枚举值 KAFKA,REDIS,MYSQL ...
        "type": "kafka",
        // 自定义 source function 名称
        "name": "kafka-source-demo",
        // 此 key 仅在 type=kafka 下有效
        "topic": [
          "aaa",
          "bbb"
        ],
        // 连接的 properties
        "props": {
          "bootstrap.servers": "localhost:9092"
        }
      },
      "sink": {
        "type": "",
        "name": "",
        "props": {

        }
      },
      // 自定义的处理函数
      "process_wrapper": "cn.lqs.flink.job_scheduler.kafka.KafkaDataProcessFunc"
    }
  ]
}